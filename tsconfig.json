{
  "compilerOptions": {
    "target": "ES6",                                  
    "module": "commonjs",                             
    "sourceMap": true,                               
    "outDir": "./dist",                        
    "noImplicitAny": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,              
    "esModuleInterop": true,
    "strict": true,                                      
    "skipLibCheck": true,          
                        
  },
  "lib": [],
  "include": [
    "./"
  ],
  "exclude": ["node_modules"]
}
